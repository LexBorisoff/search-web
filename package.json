{
  "name": "@lexjs/web-cli",
  "version": "0.0.10-beta.1",
  "description": "Node.js library and CLI for making browser web queries",
  "type": "module",
  "scripts": {
    "prebuild": "rm -fr ./bin",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "author": "Lex Borisoff <lex@borisoff.dev>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LexBorisoff/web-cli.git"
  },
  "homepage": "https://github.com/LexBorisoff/web-cli#readme",
  "bugs": {
    "url": "https://github.com/LexBorisoff/web-cli/issues"
  },
  "exports": {
    "./define-config": {
      "default": "./bin/config/define-config/index.js",
      "types": "./bin/types/config/define-config/index.d.ts"
    }
  },
  "bin": {
    "web": "bin/index.js"
  },
  "keywords": [
    "cli",
    "search",
    "websearch",
    "web-search",
    "search-engine",
    "browser-query",
    "web-query",
    "browser",
    "google"
  ],
  "files": [
    "bin"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@lexjs/browser-search": "^0.0.2",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "open": "^9.1.0",
    "prompts": "^2.4.2",
    "type-fest": "^4.15.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/node": "^18.17.6",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^3.4.1",
    "typescript": "^5.2.2"
  }
}
