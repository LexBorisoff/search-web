{
  "name": "@lexjs/web-cli",
  "version": "1.3.3",
  "description": "CLI for making browser search queries",
  "type": "module",
  "scripts": {
    "prebuild": "npm run lint:fix && rm -fr ./bin",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepack": "npm run build",
    "lint": "eslint src/**/*.{js,ts}",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Lex Borisoff <lex@borisoff.dev>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LexBorisoff/web-cli.git"
  },
  "homepage": "https://github.com/LexBorisoff/web-cli/tree/main#readme",
  "bugs": {
    "url": "https://github.com/LexBorisoff/web-cli/issues"
  },
  "exports": {
    "./config": {
      "types": "./bin/types/config/index.d.ts",
      "import": "./bin/config/index.js",
      "default": "./bin/config/index.js"
    }
  },
  "bin": {
    "web": "bin/index.js"
  },
  "keywords": [
    "cli",
    "search",
    "websearch",
    "web-search",
    "search-engine",
    "browser-query",
    "web-query",
    "browser",
    "google"
  ],
  "files": [
    "bin"
  ],
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@lexjs/web-search": "^1.1.4",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "open": "^9.1.0",
    "prompts": "^2.4.2",
    "type-fest": "^4.15.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@lexjs/eslint-plugin": "^1.1.2",
    "@types/node": "^18.17.6",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.24",
    "globals": "^15.14.0",
    "typescript": "^5.4.5"
  }
}
